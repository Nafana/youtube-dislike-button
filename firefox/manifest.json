{
    "manifest_version": 2,
    "name": "YouTube Dislike Button",
    "version": "1.0.2",
    "description": "Displays the dislike count on YouTube videos.",
    "icons": {
        "16": "/images/16x16.png",
        "32": "/images/32x32.png",
        "64": "/images/64x64.png",
        "128": "/images/128x128.png"
    },
    "author": "Nafana",
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ],
            "run_at": "document_end",
            "js": [
                "main.js"
            ]
        }
    ],
    "permissions": [
        "webRequest",
        "*://backend.nafdev.workers.dev/*"
    ],
    "browser_specific_settings": {
        "gecko": {
          "id": "{5d9f599f-b659-4379-acae-e343b8351e4c}"
        }
    }
}